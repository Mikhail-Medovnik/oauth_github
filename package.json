{
  "name": "fastify-template",
  "version": "1.0.0",
  "packageManager": "yarn@4.1.1",
  "engines": {
    "node": ">=20.9.0",
    "yarn": ">=3.0.0"
  },
  "main": "index.js",
  "type": "module",
  "exports": "./dist/index.js",
  "repository": "git@github.com:rtivital/fastify-template.git",
  "author": "Vitaly Rtishchev <rtivital@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=production node dist/index.js",
    "serve": "NODE_ENV=development node -r dotenv/config dist/index.js",
    "dev": "NODE_ENV=development nodemon --es-module-specifier-resolution=node -r dotenv/config src/index.ts",
    "clean": "rimraf dist",
    "build": "npm run clean && tsc",
    "lint": "eslint src --cache",
    "typecheck": "tsc --noEmit",
    "jest": "jest",
    "prettier:test": "prettier --check \"**/*.{ts,tsx}\"",
    "prettier:write": "prettier --write \"**/*.{ts,tsx}\"",
    "test": "npm run prettier:test && npm run typecheck && npm run lint"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.1.1",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.4",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.55.0",
    "eslint-plugin-security": "^2.1.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "pino-pretty": "^10.2.3",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@fastify/cors": "^8.4.2",
    "@fastify/formbody": "^7.4.0",
    "@fastify/helmet": "^11.1.1",
    "axios": "^1.6.8",
    "dotenv": "^16.3.1",
    "fastify": "^4.24.3",
    "fastify-html": "^0.3.3",
    "zod": "^3.22.4"
  }
}
